package SAM;

public class ThreadSAM {

	public static void main(String args[]) {
		
		//General Method
		Thread t1 = new Thread(new Runnable() {
			public void run() {
				System.out.println("In child thread ... ");
			}
		});
		t1.start();
		
		System.out.println("In main thread...");
		
		Runnable r1 = () ->{
			for(int i=0;i<=10;i++) {
				System.out.println("In child thread...");
			}
		};
		
	}
}
